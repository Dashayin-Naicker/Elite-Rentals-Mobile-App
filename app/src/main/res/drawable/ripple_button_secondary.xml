<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/brandSecondaryDisabled"/>
            <corners android:radius="@dimen/button_radius"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/brandSecondaryPressed"/>
            <corners android:radius="@dimen/button_radius"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/brandSecondary"/>
            <corners android:radius="@dimen/button_radius"/>
        </shape>
    </item>
</selector>
